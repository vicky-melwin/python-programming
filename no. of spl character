str=input("Enter the string:")
a,d,s,spl=0,0,0,0
alpha=[]
digit=[]
spl_char=[]
for i in range(len(str)):
    if str[i].isalpha():
        a=a+1
        alpha.append(str[i])
    elif(str[i].isdigit()):
        d+=1
        digit.append(str[i])
    elif(str[i].isspace()):
        s+=1
    else:
        spl+=1
        spl_char.append(str[i])
print("The number of alphabet:",a)
print(alpha)
print("The number of digits:",d)
print(digit)
print("The number of spaces:",s)
print("The number of special character:",spl)
print(spl_char)
